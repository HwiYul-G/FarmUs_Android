<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="farmDetail"
            type="com.example.farmus_application.model.farm.detail.DetailResult" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@id/bottom_menu"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <FrameLayout
                    android:id="@+id/farm_detail_frame"
                    android:layout_width="match_parent"
                    android:layout_height="329dp"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.viewpager2.widget.ViewPager2
                        android:id="@+id/farm_detail_image"
                        android:layout_width="match_parent"
                        android:layout_height="329dp"
                        android:background="@color/gray_first"
                        app:layout_constraintTop_toBottomOf="@+id/farm_detail_toolbar" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="56dp">

                        <androidx.appcompat.widget.AppCompatImageButton
                            android:id="@+id/farm_detail_bookmark_button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/heart_line_vector_image"
                            android:backgroundTint="@color/white"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@id/farm_detail_share_button"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.5" />

                        <androidx.appcompat.widget.AppCompatImageButton
                            android:id="@+id/farm_detail_share_button"
                            android:layout_width="44dp"
                            android:layout_height="44dp"
                            android:layout_marginEnd="7dp"
                            android:background="@drawable/sharing_second_vector_image"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.5" />

                        <include
                            android:id="@+id/farm_detail_toolbar"
                            layout="@layout/toolbar_without_title"
                            tools:layout_editor_absoluteX="-1dp"
                            tools:layout_editor_absoluteY="0dp" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </FrameLayout>

                <TextView
                    android:id="@+id/farm_detail_address"
                    style="@style/Body2Reg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="19dp"
                    locationBig="@{farmDetail.locationBig}"
                    locationMid="@{farmDetail.locationMid}"
                    locationSmall="@{farmDetail.locationSmall}"
                    tools:text="경기도 화성시 진안동"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/farm_detail_frame" />

                <TextView
                    android:id="@+id/farm_detail_title"
                    style="@style/Title2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="8dp"
                    android:text="@{farmDetail.name}"
                    tools:text="진안동 주말 농장"
                    android:lineSpacingExtra="11sp"
                    app:layout_constraintTop_toBottomOf="@id/farm_detail_address" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/farm_size_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="48dp"
                    android:background="@drawable/layout_line_vector_image"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/farm_detail_title" />

                <TextView
                    android:id="@+id/farm_detail_size"
                    style="@style/Body2Reg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="48dp"
                    tools:text="@string/example_farm_size"
                    squareMeters="@{farmDetail.squaredMeters}"
                    app:layout_constraintStart_toEndOf="@id/farm_size_icon"
                    app:layout_constraintTop_toBottomOf="@id/farm_detail_title" />

                <TextView
                    android:id="@+id/farm_detail_price"
                    style="@style/Title2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    tools:text="@string/example_farm_price"
                    priceFormat="@{farmDetail.price}"
                    android:textColor="@color/main_green"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/farm_size_icon" />

                <TextView
                    android:id="@+id/farm_detail_views"
                    style="@style/Body2Reg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="18dp"
                    android:text="@string/example_farm_detail_views"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.92"
                    app:layout_constraintStart_toEndOf="@id/farm_detail_price"
                    app:layout_constraintTop_toBottomOf="@id/farm_detail_size" />

                <View
                    android:id="@+id/divider_1"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginTop="24.5dp"
                    android:background="@color/gray_second"
                    app:layout_constraintTop_toBottomOf="@id/farm_detail_price" />

                <TextView
                    android:id="@+id/farm_detail_description_title"
                    style="@style/Title2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="25dp"
                    android:text="@string/farm_detail_description_title"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/divider_1" />

                <TextView
                    android:id="@+id/farm_detail_description"
                    style="@style/Body1Reg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
